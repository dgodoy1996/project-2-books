<%- include('../partials/header') %>

<h2>Books</h2>
<table>
    <thead>
        <tr>
            <th>Book Name:</th>
            <th>Author:</th>
            <th>Start Date</th>
            <th>End Date</th>
        </tr>
    </thead>
    <tbody>
        <% books.forEach(function(b) { %>
            <tr>
                <td><%= b.name %></td>
                <td><%= b.author %></td>
                <td><%= b.startDate %></td>
                <td><%= b.endDate %></td>
                <td><a href="/books/<%= b._id %>">DETAILS</a></td>
            </tr>
        <% }) %>
    </tbody>
</table>

<%- include('../partials/footer') %>